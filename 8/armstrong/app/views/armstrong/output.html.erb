<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Number</th>
        </tr>
    </thead>
    <tbody>
        <% @results.each_with_index do |number, index| %>
            <tr>
                <th scope="row">
                    <%= index + 1 %>
                </th>
                <td>
                    <%= number %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>