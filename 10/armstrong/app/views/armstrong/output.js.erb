<%= @results %>.forEach((element, index) => {
  let newRow = document.createElement("tr");
  let newIndex = document.createElement("th");
  let newNumber = document.createElement("td");
  newIndex.innerHTML = (index + 1).toString();
  newNumber.innerHTML = element.toString();
  newRow.appendChild(newIndex);
  newRow.appendChild(newNumber);
  table.appendChild(newRow);
});