<h1>Lucky Numbers from <%= @number %></h1>
<table class="table table-striped">
    <thead>
        <tr scope="row">
            <th scope="col">
                Index
            </th>
            <th scope="col">
                value
            </th>
        </tr>
    </thead>
    <tbody>
        <%- @rows.each_with_index do |cols,index| -%>    
        <tr scope="row">
            <td>
            <%= index + 1%>
            </td>
            <td>
                <%= cols%>
            </td>
        </tr>
        <%- end -%>
    </tbody>

</table>